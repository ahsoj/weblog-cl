{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "ts-node build/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "lint": "eslint --fix src test",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "db:push": "npx dotenv -e .env -- prisma db push",
    "db:seed": "npx dotenv -e .env -- prisma db seed",
    "migrate:dev": "npx dotenv -e .env -- prisma generate dev",
    "migrate:reset": "npx dotenv -e .env -- prisma generate reset",
    "prisma:generate": "npx dotenv -e .env -- prisma generate",
    "prisma:studio": "npx dotenv -e .env -- prisma studio",
    "production:build": "npx prisma generate && npx prisma generate deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "@sendgrid/mail": "^7.7.0",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.40.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.9",
    "@types/uuid": "^9.0.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
